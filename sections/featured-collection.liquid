{%- comment -%}
  This is a required section for the Shopify Theme Store.
  It is available in the "Featured Collection" section in the theme editor.

  Theme Store required settings
  - Collection

  Theme Store optional settings
  - Heading: title of the featured collection section
{%- endcomment -%}

<section class="bg-[#c4c4c433] featured-collection relative flex items-center flex-col">
  <div class="absolute -top-3 px-11 w-full max-w-740">
  <div>
    <img width="54" height="48" src="{{ 'figures-mob.png' | asset_img_url }}" />
  </div>
   </div>

  <div class="flex justify-center text-center items-center flex-col mb-7">
    {% if section.settings.title != blank %}
        <h2 class="font-montserrat max-w-380 md:max-w-740 mt-8 md:px-8 md:text-4xl text-[22px] featured-collection__title">{{ section.settings.title }}</h2>
      {% endif %}
      <div class="mt-2 border-b-[5px] border-red-600 w-[227px]"></div>
  </div>
    
  <div class="justify-center items-center flex flex-col md:justify-around ">
  
  {%- assign collection = collections[section.settings.collection] -%}

  {% for product in collection.products limit: 6 %}
  
    <a  href="{{ product.url | within: collection }}" class="featured-collection-item mx-4 xs:mx-7">
      <div class="max-w-[290px] md:max-w-740 flex flex-row items-center w-full px-2 xs:px-5 min-w-[240px] sm:min-w-740 py-4 bg-white mb-5 shadow-product rounded-rounded-3">
      {% if product.featured_image != blank %}
        {% if product.images[1]%}
        <div class="hidden md:block">
        <img class="" src="{{ product.images[1] | img_url: 'large' }}" alt="{{ product.images.last.alt | escape }}" />
      </div>

      <div class="flex justify-center md:hidden featured-collection__image min-w-[45%]">
        {% include 'responsive-image' with
          image: product.featured_image,
          max_width: 143,
          max_height: 114
        %}
      </div>
        {% else %}
        <div class="flex justify-center featured-collection__image min-w-[45%]">
          {% include 'responsive-image' with
            image: product.featured_image,
            max_width: 143,
            max_height: 114
          %}
        </div>
        {% endif %}
      {% endif %}
      <div class="w-full flex flex-col text-center">
      <p>{{ product.title | escape }}</p>
      <p>{{ product.description }}</p>
      <div class="flex flex-row justify-between">
        <span class="uppercase font-futura text-[9px] font-normal">Regular Price</span>
        <span class="uppercase font-futura text-[9px] font-normal">Easter Price</span>
      </div>
      <div class="flex flex-row justify-between">
        <div class="flex items-start">
          {{product.compare_at_price | money }}
        </div>
        <div class="flex items-end">
          {{product.price | money }}
        </div>
      </div>
    </div>
    
    </div>
    <div class="hidden relative top-[-2.5rem] md:top-[-2.5rem] -right-[4.7rem] max-w-fit sm:left-[27.2rem]">
      <svg class="w-[228px] xs:w-[253px]" height="33" viewBox="0 0 324 33" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <rect x="0.727539" y="0.86377" width="322.296" height="31.6864" fill="url(#pattern1)"/>
        <defs>
        <pattern id="pattern1" patternContentUnits="objectBoundingBox" width="1" height="1">
        <use xlink:href="#image0_1_535" transform="translate(-0.00670487) scale(0.00107736 0.00376)"/>
        </pattern>
        <image id="image0_1_535" width="930" height="250" xlink:href="data:image/png;base64,"/>
        </defs>
        </svg>
        <div class="absolute top-[0.8rem] left-7 flex">
          <span class="inline-block"><svg width="17" height="14" viewBox="0 0 14 12" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M13.1755 7.58878L5.23268 11.862C4.7273 12.1339 4.09739 11.9484 3.82842 11.4485L3.01548 9.9374C2.92561 9.77036 2.98952 9.56168 3.15838 9.47084C3.66376 9.19894 3.85607 8.57107 3.58711 8.07113C3.31814 7.57119 2.68823 7.38568 2.18285 7.65757C2.014 7.74844 1.80465 7.68678 1.71478 7.51974L0.90183 6.00868C0.632862 5.50874 0.825175 4.88087 1.33055 4.60898L9.27341 0.33573C9.77879 0.063838 10.4087 0.249351 10.6777 0.749292L11.4906 2.26035C11.5805 2.42739 11.5166 2.63607 11.3477 2.72691C10.8423 2.99881 10.65 3.62668 10.919 4.12662C11.188 4.62656 11.8179 4.81207 12.3232 4.54018C12.4921 4.44933 12.7015 4.51099 12.7913 4.67803L13.6043 6.18909C13.8732 6.68902 13.6809 7.31688 13.1755 7.58878ZM3.77271 9.8805L4.43942 11.1197C4.52912 11.2865 4.73895 11.3483 4.9075 11.2576L12.8504 6.98435C13.0189 6.89367 13.083 6.68452 12.9933 6.51778L12.3266 5.27854C11.5547 5.51268 10.7006 5.18517 10.308 4.45531C9.91532 3.72545 10.1126 2.83235 10.7334 2.31723L10.0667 1.07801C9.97697 0.911263 9.76714 0.849469 9.59859 0.940151L1.65573 5.2134C1.48717 5.30408 1.42312 5.51323 1.51282 5.67997L2.17953 6.91921C2.95142 6.68507 3.80545 7.01258 4.19811 7.74244C4.59077 8.4723 4.39344 9.36542 3.77271 9.8805Z" fill="white"/>
            <path d="M7.29208 9.05316L6.52391 3.23374L7.21016 3.13819L7.97834 8.9576L7.29208 9.05316Z" fill="white"/>
            <path d="M5.86258 6.97814C5.3572 7.25003 4.72729 7.06452 4.45832 6.56458C4.18935 6.06464 4.38166 5.43677 4.88704 5.16488C5.39242 4.89298 6.02233 5.0785 6.2913 5.57844C6.56026 6.07838 6.36795 6.70625 5.86258 6.97814ZM5.21222 5.7693C5.04366 5.85998 4.9796 6.06912 5.06931 6.23586C5.15902 6.40261 5.36884 6.4644 5.5374 6.37372C5.70595 6.28304 5.77001 6.07389 5.6803 5.90715C5.5906 5.74041 5.38077 5.67861 5.21222 5.7693Z" fill="white"/>
            <path d="M10.0287 7.15354C9.52334 7.42543 8.89343 7.23992 8.62446 6.73998C8.3555 6.24004 8.54781 5.61217 9.05319 5.34028C9.55856 5.06839 10.1885 5.2539 10.4574 5.75384C10.7264 6.25378 10.5341 6.88165 10.0287 7.15354ZM9.37836 5.9447C9.20981 6.03538 9.14575 6.24452 9.23546 6.41127C9.32516 6.57801 9.53499 6.6398 9.70354 6.54912C9.8721 6.45844 9.93616 6.2493 9.84645 6.08255C9.75674 5.91581 9.54692 5.85402 9.37836 5.9447Z" fill="white"/>
            </svg>
          </span>
            <span class="inline-block text-[9px] text-white">USE CODE <span class="text-[9px] text-white font-extrabold ">EASTER</span> FOR AN EXTRA 10% OFF</span>
        </div>
    </div>

    </a>
  {% else %}
    {% for i in (1..6) %}
      <a href="#" class="featured-collection-item">
        {% capture current %}{% cycle 1, 2, 3, 4, 5, 6 %}{% endcapture %}
        {{ 'product-' | append: current | placeholder_svg_tag: 'placeholder-svg placeholder-svg--small' }}

        <p>{{ 'homepage.onboarding.product_title' | t }}</p>
        <p>{{ 1999 | money }}</p>
      </a>
    {% endfor %}
  {% endfor %}
    </div>
</section>

{% schema %}
  {
    "name": "Featured collection",
    "settings": [
      {
        "type": "textarea",
        "id": "title",
        "label": "Heading",
        "default": "DON'T WAIT FOR THE EASTER BUNNY TO BRING YOURS! <br/><span class='md:text-[40px] font-black font-montserrat md:font-futura md:font-bold'>ORDER NOOCUBE TODAY!</span>"
      },
      {
        "id": "collection",
        "type": "collection",
        "label": "Collection"
      }
    ],
    "presets": [
      {
        "name": "Featured collection",
        "category": "Collection"
      }
    ]
  }
{% endschema %}
